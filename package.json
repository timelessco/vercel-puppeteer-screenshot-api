{
	"name": "vercel-puppeteer-screenshot-api",
	"version": "6.1.0",
	"private": true,
	"description": "Vercel function for screenshoting URLs using headless browsers",
	"keywords": [
		"react",
		"typescript",
		"nextjs",
		"tailwindcss"
	],
	"bugs": {
		"url": "https://github.com/timelessco/vercel-puppeteer-screenshot-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/vercel-puppeteer-screenshot-api.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true pnpm build && pnpm start",
		"build:ci": "cross-env SKIP_ENV_VALIDATION=true pnpm build",
		"build:icons": "node --experimental-strip-types scripts/build-icons.ts",
		"build:local": "cross-env LOCAL=true pnpm build --turbopack",
		"build:sourcemap": "cross-env SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"build:start": "pnpm build && pnpm start",
		"check:engine": "npx installed-check --fix",
		"check:packages": "pnpm dedupe --check",
		"check:update": "npx npm-check-updates --interactive --format group --target patch",
		"clean": "node --experimental-strip-types scripts/clean.ts",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate",
		"dev": "next dev --turbopack",
		"dev:sourcemap": "cross-env SOURCEMAP=true pnpm dev",
		"fix": "turbo fix:eslint",
		"fix:css": "stylelint --fix --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"fix:eslint": "eslint --cache --color --fix .",
		"fix:md": "markdownlint-cli2 --fix --rules sentences-per-line",
		"fix:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"fix:spelling": "node --experimental-strip-types scripts/update-cspell-words.ts",
		"lint": "turbo lint:prettier lint:eslint lint:types lint:knip lint:md lint:spelling lint:css",
		"lint:css": "stylelint --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"lint:eslint": "eslint --cache --color .",
		"lint:knip": "knip --production",
		"lint:md": "markdownlint-cli2 --rules sentences-per-line",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress --show-context check \"**/*\"",
		"lint:types": "tsc --noEmit",
		"prepare": "is-ci || husky",
		"release": "release-it --ci",
		"start": "next start"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown",
			"node --experimental-strip-types scripts/update-cspell-words.ts"
		]
	},
	"dependencies": {
		"@ariakit/react": "0.4.18",
		"@duckduckgo/autoconsent": "14.14.0",
		"@ghostery/adblocker-puppeteer": "2.11.3",
		"@serwist/next": "9.1.1",
		"@sparticuz/chromium": "138.0.2",
		"clsx": "2.1.1",
		"cross-fetch": "4.1.0",
		"get-video-id": "4.1.7",
		"ky": "1.8.2",
		"next": "15.5.7",
		"puppeteer-core": "24.16.2",
		"puppeteer-extra": "3.3.6",
		"puppeteer-extra-plugin-stealth": "2.11.2",
		"puppeteer-extra-plugin-user-data-dir": "2.4.1",
		"puppeteer-extra-plugin-user-preferences": "2.4.1",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"rebrowser-puppeteer-core": "24.8.1",
		"serwist": "9.1.1",
		"tailwind-merge": "3.3.1",
		"zod": "4.0.17"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"@effect/cli": "0.69.2",
		"@effect/platform": "0.90.5",
		"@effect/platform-node": "0.96.0",
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint-react/eslint-plugin": "1.52.6",
		"@eslint/compat": "1.3.2",
		"@eslint/js": "9.33.0",
		"@ianvs/prettier-plugin-sort-imports": "4.6.2",
		"@inquirer/prompts": "7.8.3",
		"@next/bundle-analyzer": "15.5.7",
		"@next/eslint-plugin-next": "15.5.7",
		"@octokit/core": "7.0.3",
		"@release-it/conventional-changelog": "10.0.1",
		"@sentry/nextjs": "10.5.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/postcss": "4.1.12",
		"@types/fs-extra": "11.0.4",
		"@types/node": "24.3.0",
		"@types/react": "19.1.10",
		"@types/react-dom": "19.1.7",
		"@typescript-eslint/parser": "8.40.0",
		"all-contributors-cli": "6.26.1",
		"cross-env": "10.0.0",
		"cspell": "9.2.0",
		"dotenv": "17.2.1",
		"effect": "3.17.8",
		"eslint": "9.33.0",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsdoc": "54.1.1",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-n": "17.21.3",
		"eslint-plugin-package-json": "0.54.0",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"eslint-plugin-regexp": "2.10.0",
		"eslint-plugin-unicorn": "60.0.0",
		"eslint-plugin-yml": "1.18.0",
		"execa": "9.6.0",
		"fs-extra": "11.3.1",
		"git-remote-origin-url": "4.0.0",
		"git-url-parse": "16.1.0",
		"glob": "11.1.0",
		"husky": "9.1.7",
		"is-ci": "4.1.0",
		"knip": "5.62.0",
		"lint-staged": "16.1.5",
		"markdownlint": "0.38.0",
		"markdownlint-cli2": "0.18.1",
		"node-html-parser": "7.0.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-curly": "0.3.2",
		"prettier-plugin-packagejson": "2.5.19",
		"prettier-plugin-sh": "0.18.0",
		"prettier-plugin-tailwindcss": "0.6.14",
		"rebrowser-puppeteer": "24.8.1",
		"release-it": "19.0.4",
		"sentences-per-line": "0.3.0",
		"sharp": "0.34.3",
		"source-map-explorer": "2.5.3",
		"stylelint": "16.23.1",
		"stylelint-config-clean-order": "7.0.0",
		"stylelint-config-standard": "39.0.0",
		"svgo": "4.0.0",
		"tailwindcss": "4.1.12",
		"turbo": "2.5.6",
		"typescript": "5.9.2",
		"typescript-eslint": "8.40.0"
	},
	"packageManager": "pnpm@10.15.0",
	"engines": {
		"node": "^20.19.0 || ^22.12.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@sentry/cli",
			"@tailwindcss/oxide",
			"esbuild",
			"msgpackr-extract",
			"oxc-resolver",
			"puppeteer",
			"rebrowser-puppeteer",
			"sharp",
			"unrs-resolver"
		],
		"overrides": {
			"@types/react": "19.1.10",
			"@types/react-dom": "19.1.7"
		}
	}
}
